  "reducer effect definition": {
    "prefix": "reducereffectdefinition",
    "body": [
      "readonly effects: (EffectState | { value: EffectValueNoEffect }) & {",
      "\tcontext: EffectContext;",
      "};",
      "",
      "export const StateValue = {",
      "\teffectValNoEffect: \"noEffect\" as EffectValueNoEffect,",
      "\teffectValHasEffects: \"hasEffects\" as EffectValueHasEffects,",
      "};",
      "",
      "function prepareToAddEffect(globalState: StateMachine) {",
      "\tconst effects = (globalState.effects as unknown) as EffectState;",
      "\teffects.value = StateValue.effectValNoEffect;",
      "\tconst effectObjects: EffectObject = [];",
      "\teffects.hasEffects = {",
      "\t\t\tcontext: {",
      "\t\t\t\t\teffects: effectObjects,",
      "\t\t\t},",
      "\t};",
      "",
      "\treturn effectObjects;",
      "}",
      "",
      "export interface EffectFunctionsArgs {",
      "\ta: string;",
      "}",
      "type EffectValueNoEffect = \"noEffect\";",
      "type EffectValueHasEffects = \"hasEffects\";",
      "",
      "interface EffectContext {",
      "\tmetaFunctions: EffectFunctionsArgs;",
      "}",
      "",
      "type EffectObject = []",
      "",
      "interface EffectState {",
      "\tvalue: EffectValueHasEffects;",
      "\thasEffects: {",
      "\t\t\tcontext: {",
      "\t\t\t\t\teffects: EffectObject;",
      "\t\t\t};",
      "\t};",
      "}",
      "",
      "interface ${1:component_name}EffectDefinition<",
      "\tKey extends string,",
      "\tEffectArgKeys extends keyof EffectFunctionsArgs,",
      "\tOwnArgs = {}",
      "> {",
      "\tkey: Key;",
      "\teffectArgKeys: EffectArgKeys[];",
      "\townArgs: OwnArgs;",
      "\tfunc: (",
      "\t\t\teffectArgs: { [k in EffectArgKeys]: EffectFunctionsArgs[k] },",
      "\t\t\townArgs: OwnArgs,",
      "\t) => void | Promise<void>;",
      "}",
      "",
      "useEffect(() => {",
      "\tif (effects.value === StateValue.effectValHasEffects) {",
      "\t\t\tconst {",
      "\t\t\t\t\tcontext: { metaFunctions },",
      "\t\t\t\t\thasEffects: { context },",
      "\t\t\t} = effects;",
      "",
      "\t\t\tfor (const { func, ownArgs, effectArgKeys } of context.effects) {",
      "\t\t\t\t\tconst effectArgKeys1 = effectArgKeys as (keyof EffectFunctionsArgs)[];",
      "\t\t\t\t\tconst args = effectArgKeys1.reduce(",
      "\t\t\t\t\t\t\t(acc, k) => {",
      "\t\t\t\t\t\t\t\t\tacc[k] = metaFunctions[k];",
      "\t\t\t\t\t\t\t\t\treturn acc;",
      "\t\t\t\t\t\t\t},",
      "\t\t\t\t\t\t\t{} as EffectFunctionsArgs,",
      "\t\t\t\t\t);",
      "",
      "\t\t\t\t\t/* eslint-disable-next-line @typescript-eslint/no-explicit-any*/",
      "\t\t\t\t\tfunc(args, ownArgs as any);",
      "\t\t\t}",
      "\t}",
      "}, [effects]);"
    ],
    "description": "reducer effect definition"
  },
  "state update effect definition": {
    "prefix": "stateupdateeffectdefinition",
    "body": [
      "{",
      "type: ActionType.PUT_EFFECT_FUNCTIONS_ARGS;",
      "} & EffectFunctionsArgs",
      "\n",
      "PUT_EFFECT_FUNCTIONS_ARGS = \"@${2:component_path}/put-effects-functions-args\"",
      "\n",
      "const effectFunctions = {}",
      "",
      "readonly effects: (EffectState | { value: EffectValueNoEffect }) & {",
      "\tcontext: EffectContext;",
      "};",
      "",
      "export const StateValue = {",
      "\teffectValNoEffect: \"noEffect\" as EffectValueNoEffect,",
      "\teffectValHasEffects: \"hasEffects\" as EffectValueHasEffects,",
      "};",
      "",
      "function prepareToAddEffect(globalState: StateMachine) {",
      "\tconst effects = (globalState.effects as unknown) as EffectState;",
      "\teffects.value = StateValue.effectValNoEffect;",
      "\tconst effectObjects: EffectObject = [];",
      "\teffects.hasEffects = {",
      "\t\t\tcontext: {",
      "\t\t\t\t\teffects: effectObjects,",
      "\t\t\t},",
      "\t};",
      "",
      "\treturn effectObjects;",
      "}",
      "",
      "export interface EffectFunctionsArgs {",
      "\ta: string;",
      "}",
      "",
      "type EffectValueNoEffect = \"noEffect\";",
      "type EffectValueHasEffects = \"hasEffects\";",
      "",
      "interface EffectContext {",
      "\tmetaFunctions: EffectFunctionsArgs;",
      "}",
      "",
      "type EffectObject = []",
      "",
      "interface EffectState {",
      "\tvalue: EffectValueHasEffects;",
      "\thasEffects: {",
      "\t\t\tcontext: {",
      "\t\t\t\t\teffects: EffectObject;",
      "\t\t\t};",
      "\t};",
      "}",
      "",
      "interface ${1:component_name}EffectDefinition<",
      "\tKey extends keyof typeof effectFunctions,",
      "\tEffectArgKeys extends keyof EffectFunctionsArgs,",
      "\tOwnArgs = {}",
      "> {",
      "\tkey: Key;",
      "\teffectArgKeys: EffectArgKeys[];",
      "\townArgs: OwnArgs;",
      "\tfunc?: (",
      "\t\t\teffectArgs: { [k in EffectArgKeys]: EffectFunctionsArgs[k] },",
      "\t\t\townArgs: OwnArgs,",
      "\t) => void | Promise<void | (() => void)> | (() => void);",
      "}",
      "",
      "useEffect(() => {",
      "\tif (effects.value !== StateValue.effectValHasEffects) {",
      "\t\t\treturn;",
      "\t}",
      "",
      "\tconst {",
      "\t\t\tcontext: { metaFunctions },",
      "\t\t\thasEffects: { context },",
      "\t} = effects;",
      "",
      "\tconst cleanupEffects: (() => void)[] = [];",
      "",
      "\t(async function runEffects() {",
      "\t\t\tfor (const { key, ownArgs, effectArgKeys } of context.effects) {",
      "\t\t\t\t\tconst args = (effectArgKeys as (keyof EffectFunctionsArgs)[]).reduce(",
      "\t\t\t\t\t\t\t(acc, k) => {",
      "\t\t\t\t\t\t\t\t\tacc[k] = metaFunctions[k];",
      "\t\t\t\t\t\t\t\t\treturn acc;",
      "\t\t\t\t\t\t\t},",
      "\t\t\t\t\t\t\t{} as EffectFunctionsArgs,",
      "\t\t\t\t\t);",
      "",
      "\t\t\t\t\tconst maybeCleanupEffect = await effectFunctions[key](",
      "\t\t\t\t\t\t\targs,",
      "\t\t\t\t\t\t\t/* eslint-disable-next-line @typescript-eslint/no-explicit-any*/",
      "\t\t\t\t\t\t\townArgs as any,",
      "\t\t\t\t\t);",
      "",
      "\t\t\t\t\tif (\"function\" === typeof maybeCleanupEffect) {",
      "\t\t\t\t\t\t\tcleanupEffects.push(maybeCleanupEffect);",
      "\t\t\t\t\t}",
      "\t\t\t}",
      "\t})();",
      "",
      "\tif (cleanupEffects.length) {",
      "\t\t\treturn () => {",
      "\t\t\t\t\tcleanupEffects.forEach(f => f());",
      "\t\t\t};",
      "\t}",
      "",
      "\t// redundant - [tsserver 7030] [W] Not all code paths return a value.",
      "\treturn;",
      "}, [effects]);"
    ],
    "description": "state update effect definition"
  }
